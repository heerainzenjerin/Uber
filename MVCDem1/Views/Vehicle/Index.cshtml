
@{
    ViewBag.Title = "Index";
}


<div class="col-md-4 col-md-offset-3">
    <form method="post">
        <label for="Message">@ViewBag.Message </label>

        <div class="form-group">
            <label for="DriverName">Driver Name </label>
            <input type="text" id="DriverName" class="form-control" required name="DriverName" />
        </div>

        <div class="form-group">
            <label for="vechicleNo">vechicle No :</label>
            <input type="text" id="password" class="form-control" required name="vechicleNo" />
        </div>

        <div class="form-group">
            <label for="vechicleModel">vechicle Model:</label>
            <input type="text" id="vechicleModel" class="form-control" required name="vechicleModel" />
        </div>
        <div class="form-group">
            <label for="Rating">Rating :</label>
            <input type="text" id="Rating" class="form-control" required name="Rating" />
        </div>


        <div class="form-group">

            <input type="submit" class="btn btn-primary" />
        </div>

    </form>



</div>

